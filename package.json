{
  "name": "@newjersey/stencil-react-export",
  "workspaces": [
    "packages/*"
  ],
  "module": "packages/stencil-library/dist/index.js",
  "types": "packages/stencil-library/dist/types/index.d.ts",
  "collection": "packages/stencil-library/dist/collection/collection-manifest.json",
  "collection:main": "packages/stencil-library/dist/collection/index.js",
  "main": "packages/stencil-library/dist/index.cjs.js",
  "unpkg": "packages/stencil-library/dist/stencil-library/stencil-library.esm.js",
  "exports": {
    ".": {
      "import": "./packages/stencil-library/dist/stencil-library/stencil-library.esm.js",
      "require": "./packages/stencil-library/dist/stencil-library/stencil-library.cjs.js"
    },
    "./my-component": {
      "import": "./packages/stencil-library/dist/components/my-component.js",
      "types": "./packages/stencil-library/dist/components/my-component.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newjersey/stencil-react-export.git"
  },
  "files": [
    "packages/stencil-library/dist/",
    "packages/stencil-library/loader/",
    "packages/react-library/dist/"
  ],
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "lerna": "^8.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.3"
  },
  "version": "0.0.11"
}
